function TRSMinimap.IsPoisoned: Boolean;
var
  B: TBox;
begin
  with Minimap.OrbInfo[MMOrbHP] do
    B := [Middle.X - Radius, Middle.Y - Radius, Middle.X + Radius, Middle.Y + Radius];

  Result := CountColorTolerance(101136, B, 85) > 0;
end;

