  with (Self.OrbInfo[Ord(MMOrbRun)]) do
  begin
    Mouse.Click(Middle, Radius, mouse_Left);
    t.Init(1000);
    while (not t.IsFinished()) and (Self.isRunEnabled() <> Enable) do
      Wait(40);
    Result := Self.isRunEnabled() = Enable;
  end;
end;

